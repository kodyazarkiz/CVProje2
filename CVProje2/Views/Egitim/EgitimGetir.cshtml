@model CVProje2.Models.entity.TblEğitimDeneyim
@{
    ViewBag.Title = "EgitimGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />

<h4 style="margin-left: 10px; margin-top: 10px; margin-bottom: 30px">Eğitim Bilgisi Güncelleme Sayfası</h4>
<br />
<br />
<div class="container-fluid">
    <form method="post">
        @Html.Label("Eğitim ID")
        @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
        <br />
        @Html.Label("Başlık")
        @Html.TextBoxFor(x => x.EBaslik, new { @class = "form-control" })
        <br />
        @Html.Label("Tarih")
        @Html.TextBoxFor(x => x.ETarih, new { @class = "form-control" })
        <br />
        @Html.Label("GNO")
        @Html.TextBoxFor(x => x.GNO, new { @class = "form-control" })
        <br />
        @Html.Label("Açıklama")
        @Html.TextAreaFor(x => x.EAciklama, new { @class = "form-control" })
        <br />
        <button class="btn btn-outline-warning">Güncelle</button>
    </form>
</div>
