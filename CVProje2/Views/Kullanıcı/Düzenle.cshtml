@model CVProje2.Models.entity.TblAdmin
@{
    ViewBag.Title = "Düzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <br />
    <h4>Kullanıcı Bilgisi Düzenleme Sayfası</h4>
    <br />
    <form method="post">
        @Html.Label("Kullanıcı Adı")
        @Html.TextBoxFor(x => x.KullaniciAdi, new { @class = "form-control" })
        <br />
        @Html.Label("Şifre")
        @Html.TextBoxFor(x => x.Sifre, new { @class = "form-control" })
        <br />
        <button class="btn btn-outline-primary">Güncelle</button>
    </form>
</div>

